@model List<RMS.Models.Room>
@{
    
    Layout = null;
    int temporada = ViewBag.Temporada;
    int hotel = ViewBag.Hotel;
    
}
<!DOCTYPE html>
<html>
<head>
    <title>PrintableRoom</title>
</head>
<body>
    <div>
        @switch (temporada)
        {
            case 1:
                Html.RenderPartial("RoomRateHigh", Model);
                break;
            case 2:
                Html.RenderPartial("RoomRate", Model);
                break;
            case 3:
                Html.RenderPartial("RoomRatePromotion", Model);
                break;
        }
    </div>
</body>
</html>
