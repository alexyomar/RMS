    <script type="text/javascript">
        var now = 0;
        var int = self.setInterval("changeBG()", 10000);

        var array = ["/Fondos/002.jpg", "/Fondos/003.jpg", "/Fondos/004.jpg", "/Fondos/005.jpg", "/Fondos/006.jpg", "/Fondos/007.jpg", "/Fondos/008.jpg", "/Fondos/001.jpg"];
        var divactive = "#background";
        var divbg = "#background2";
        function changeBG() {

            now = (now + 1) % array.length;
            $(divbg).css('background-image', 'url("' + array[now] + '")');
            $(divbg).show();
            $(divactive).fadeOut(2000);
            $(this).dequeue();

            if (divbg == "#background2") {
                divbg = "#background";
                divactive = "#background2"
            }
            else {
                divactive = "#background";
                divbg = "#background2"
            }
            $(divactive).css('z-index', '0');
            $(divbg).css('z-index', '1');
        }
    </script>
